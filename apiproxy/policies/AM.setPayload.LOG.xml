<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM.setPayload.LOG">
    <DisplayName>AM.setPayload.LOG</DisplayName>
    <Set>
        <Headers>
            <Header name="user-agent">{request.header.user-agent}</Header>
        </Headers>
        <Payload contentType="text/plain">Apigee-GCP Services: Logs sent to GCP Logging (check https://console.cloud.google.com/logs/viewer, logName="projects/{gcp.projectid}/logs/{gcplogging.logid}").</Payload>
        <Path/>
        <StatusCode>200</StatusCode>
        <Verb>GET</Verb>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>