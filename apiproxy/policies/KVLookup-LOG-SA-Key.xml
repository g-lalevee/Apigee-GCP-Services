<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations continueOnError="false" enabled="true" name="KVLookup-LOG-SA-Key" mapIdentifier="GCP-SA-settings">
    <DisplayName>KV.Lookup-LOG-SA-Key</DisplayName>
    <Scope>environment</Scope>
    <ExpiryTimeInSecs>30</ExpiryTimeInSecs>
    <!-- these gets will be cached -->
    <Get assignTo="private.gcp.service_account.key">
        <Key>
            <Parameter>MyLOG</Parameter>
            <Parameter>privKeyPem</Parameter>
        </Key>
    </Get>
</KeyValueMapOperations>