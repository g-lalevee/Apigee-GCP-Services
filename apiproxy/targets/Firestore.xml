<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="Firestore">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AMsetTargetPath</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>https://firestore.googleapis.com/v1/projects/{gcp.projectid}/databases/(default)/documents/{collection.name}</URL>
    </HTTPTargetConnection>
</TargetEndpoint>